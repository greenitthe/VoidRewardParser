<Application x:Class="VoidRewardParser.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:VoidRewardParser"
             xmlns:converters="clr-namespace:VoidRewardParser.Converters"
             StartupUri="MainWindow.xaml">
    <Application.Resources>
        <ResourceDictionary>
            <converters:BooleanToVisibilityConverter x:Key="BooleanToVilibiltyConverter"/>
            <converters:RarityToColorConverter x:Key="RarityToColorConverter"/>
            <converters:LocalizedStringConverter x:Key="LocalizedStringConverter"/>
            <converters:DucatRatioColorConverter x:Key="DucatRatioColorConverter"/>
        </ResourceDictionary>
    </Application.Resources>
</Application>
